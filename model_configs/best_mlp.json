{
 "hyperparams": {
    "embedding_dropout":0.0,
    "batch_norm_continuous_input":false,
    "hidden_layers":"1024-512-256-128",
    "activation":"leaky_relu",
    "dropout":0.2  
 },
 "train_params": {
     "init_bias": false,
     "loss" : {
       "func": "focal",
       "gamma": 5.0,
       "beta": 0.999,
       "alpha": 1.0,
       "label_smoothing":0.1
     },
     "optimizer": {
         "func": "Adam",
         "lr":0.1,
         "decay": 0.0
     },

     "scheduler": {
         "func": "CosineAnnealingLR",
         "Tmax": 200
     },

     "accelerator": "gpu",
     "max_epochs": 200,
     "batch_size": 2048,
     "num_workers": 10,
     "pin_memory": true
 }
}
